<% provide(:title, '出版社登録') %>
<h1>出版社登録</h1>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <%= form_for(@publisher) do |f| %>
            <%= f.label '出版社' %>
            <%= f.text_field :name %>
        
            <%= f.submit "登録", class: "btn btn-primary" %>
        <% end %>
    </div>
</div>

